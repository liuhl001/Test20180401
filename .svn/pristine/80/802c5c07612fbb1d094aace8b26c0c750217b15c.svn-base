<?xml version="1.0" encoding="utf-8"?>
<suite name="测试" parallel="classes" thread-count="3">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="注册" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Register" />

		</classes>
	</test>
	<test name="登录" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Login" />

		</classes>
	</test>

	<test name="日期 格式" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Datetimetest" />

		</classes>
	</test>
	<test name="按车次添加" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Addtrain" />

		</classes>
	</test>
	<test name="按行程添加" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Addtrain2" />

		</classes>
	</test>
	 <test name="初始化车站测试数据" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Trainstation" />

		</classes>
	</test>

	<test name="app添加关注" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Addtrainstatue" />

		</classes>
	</test>
	<test name="app关注状态验证" preserve-order="true">
		<classes>
			<class name="com.winsion.inception.test.Testtrainstatue" />

		</classes>
	</test>

</suite>
